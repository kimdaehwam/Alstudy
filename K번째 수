#include <stdio.h>#include <iostream>#include <vector>#include <algorithm>#include <functional>using namespace std;int main(){ int N, K; cin >> N >> K; vector<int> v(N); for (int i = 0;i <= N - 1;i++)  scanf("%d", &v[i]); nth_element(v.begin(), v.begin() + K-1, v.end()); printf("%d", v[K - 1]);  return 0;}
