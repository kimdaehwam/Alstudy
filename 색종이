#include <stdio.h>#include <iostream>#include <string.h>using namespace std;int main(){ int N; cin >> N; int square[101][101]; int sum = 0; memset(square, 0, sizeof(square)); while (N--) {  int x, y;  cin >> x >> y;  for (int i = y;i <= y + 9;i++)  {   for (int j = x;j <= x + 9;j++)    square[i][j] = 1;  } } for (int i = 1;i <= 100;i++) {  for (int j = 1;j <= 100;j++)   sum = sum + square[i][j]; } cout << sum; return 0;}
