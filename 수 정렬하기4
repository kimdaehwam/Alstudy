#include <stdio.h>
#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main()
{
	int N;
	scanf("%d", &N);
	vector<int> v;
	v.resize(N);
	for (int i = 0;i <= N - 1;i++)
		scanf("%d", &v[i]);
	sort(v.begin(), v.end());
	for (int j = N - 1;j >= 0;j--)
		printf("%d\n", v[j]);

	return 0;
}
