#include <stdio.h>
#include <iostream>
#include <algorithm>
 
using namespace std;
 
int main()
{
    int N, arr[1000001];
    cin >> N;
    for (int i = 1;i <= N;i++)
        cin >> arr[i];
    sort(arr + 1, arr + N + 1);
    cout << arr[1] << " " << arr[N] << endl;
 
    return 0;
}

int main()
{
	scanf("%d", &N);
	for (int i = 1;i <= N;i++)
		scanf("%d", &arr[i]);
	sort(arr + 1, arr + N+1);
	int M;
	scanf("%d", &M);
	while (M--)
	{
		scanf("%d", &target);
		binarysearch(target);
	}

	return 0;
}
