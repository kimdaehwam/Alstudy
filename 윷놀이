#include <stdio.h>
#include <iostream>

using namespace std;

int arr[4];
int main()
{
	for (int j = 1;j <= 3;j++)
	{
		for (int i = 0;i <= 3;i++)
		{
			cin >> arr[i]; 
		}
	
	if (arr[0] + arr[1] + arr[2] + arr[3] == 3)
		printf("%c\n", 'A');
	if (arr[0] + arr[1] + arr[2] + arr[3] == 2)
		printf("%c\n", 'B');
	if (arr[0] + arr[1] + arr[2] + arr[3] == 1)
		printf("%c\n", 'C');
	if (arr[0] + arr[1] + arr[2] + arr[3] == 0)
		printf("%c\n", 'D');
	if (arr[0] + arr[1] + arr[2] + arr[3] == 4)
		printf("%c\n", 'E');
	}

	return 0;
}
