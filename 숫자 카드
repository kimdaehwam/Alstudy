#include <stdio.h>#include <iostream>#include <vector>#include <algorithm>using namespace std;int N, M;vector<int> card;vector<int> target;bool binarysearch(int i){ int low = 0; int high = N-1; int mid; while (low <= high) {  mid = (low + high) / 2;  if (card[mid] == target[i])   return true;  else if (card[mid] < target[i])   low = mid + 1;  else if (card[mid] > target[i])   high = mid - 1; } return false;}int main(){ cin >> N; card.resize(N); for (int i = 0;i <= N - 1;i++)  scanf("%d", &card[i]); cin >> M; target.resize(M); for (int i = 0;i <= M - 1;i++)  scanf("%d", &target[i]); sort(card.begin(), card.end()); for (int i = 0;i <= M - 1;i++)  printf("%d ", binarysearch(i));  return 0;}
