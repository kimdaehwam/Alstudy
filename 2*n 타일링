#include <stdio.h>#include <iostream>#include <string.h>using namespace std;long long memo[1001];int solve(int N){ if (N <= 0)  return 0; if (N == 1)  return 1; if (N == 2)  return 2; long long &ret = memo[N]; if (ret != -1)  return ret; ret = (solve(N - 1) + solve(N - 2)) % 10007; return ret;}int main(){ int N; cin >> N; memset(memo, -1, sizeof(memo)); printf("%ld", solve(N)); return 0;}
